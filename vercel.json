{
    "version": 2,
    "builds": [
        {
            "src": "*.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/admin",
            "dest": "/admin.html"
        },
        {
            "src": "/",
            "dest": "/index.html"
        },
        {
            "handle": "filesystem"
        }
    ]
}